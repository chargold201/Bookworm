<%= form_with model: @book, local: true do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :author %>
  <%= f.text_field :author %>
  <br>
  <%= f.label :genre_id %>
  <%= f.collection_select(:genre_id, Genre.all, :id, :name) %>
  <br>
  <%= f.label :isbn %>
  <%= f.text_field :isbn %>
  <br>
  <%= f.submit %>
<% end %>